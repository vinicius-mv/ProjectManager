@if (Ticket != null)
{
    <tr>
        <td>@Ticket.Title</td>
        <td>@Ticket.Owner</td>
        <td>@(Ticket.ReportDate?.ToString("yyyy-MM-dd") ?? string.Empty)</td>
        <td>@(Ticket.DueDate?.ToString("yyyy-MM-dd") ?? string.Empty)</td>
    </tr>
}

@code {
    [Parameter]
    public Ticket Ticket { get; set; }
}
